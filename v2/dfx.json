{
  "canisters": {
    "notifications": {
      "type": "custom",
      "candid": "backend/canisters/notifications/api/candid/c2c.did",
      "wasm": "target/wasm32-unknown-unknown/release/notifications_canister_impl-opt.wasm",
      "build": "./generate-wasm.sh notifications"
    },
    "user_index": {
      "type": "custom",
      "candid": "backend/canisters/user_index/api/candid/app.did",
      "wasm": "target/wasm32-unknown-unknown/release/user_index_canister_impl-opt.wasm",
      "build": "./generate-wasm.sh user_index"
    },
    "user": {
      "type": "custom",
      "candid": "backend/canisters/user/api/candid/app.did",
      "wasm": "target/wasm32-unknown-unknown/release/user_canister_impl-opt.wasm",
      "build": "./generate-wasm.sh user"
    },
    "website": {
      "frontend": {
        "entrypoint": "frontend/public/index.html"
      },
      "source": ["frontend/public"],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "ic": {
      "bind": "https://ic0.app/",
      "type": "persistent"
    }
  },
  "version": 1
}
