{
  "canisters": {
    "notifications": {
      "type": "custom",
      "candid": "backend/canisters/notifications/src/canister/c2c/can.did",
      "wasm": "target/wasm32-unknown-unknown/release/notifications-opt.wasm",
      "build": "./generate-wasm.sh notifications"
    },
    "user_index": {
      "type": "custom",
      "candid": "backend/canisters/user_index/src/canister/app/can.did",
      "wasm": "target/wasm32-unknown-unknown/release/user_index-opt.wasm",
      "build": "./generate-wasm.sh user_index"
    },
    "user": {
      "type": "custom",
      "candid": "backend/canisters/user/src/canister/app/can.did",
      "wasm": "target/wasm32-unknown-unknown/release/user-opt.wasm",
      "build": "./generate-wasm.sh user"
    },
    "website": {
      "frontend": {
        "entrypoint": "frontend/public/index.html"
      },
      "source": ["frontend/public"],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}
