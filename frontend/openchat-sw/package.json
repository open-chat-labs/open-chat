{
    "name": "openchat-sw",
    "version": "1.0.0",
    "description": "OpenChat custom service worker and loading script",
    "private": true,
    "scripts": {
        "build": "sh ./build_dev.sh",
        "build:prod": "sh ./build_prod.sh && ./bin/portable-webpack",
        "build:prod_test": "sh ./build_prod_test.sh && ./bin/portable-webpack",
        "build:sw": "./bin/portable-webpack",
        "build:sw-dev": "FORCE_FETCH_ROOT_KEY=1 ./bin/portable-webpack --mode development",
        "dev": "npm run build:sw-dev && sh ./build_dev.sh -w"
